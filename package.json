{
  "name": "vigy-store",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "predeploy": "medusa db:migrate",
    "build": "medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "start:pre": "dotenv -e .env.pre -- cross-env NODE_ENV=development medusa start",
    "start:pro": "dotenv -e .env.pro -- cross-env NODE_ENV=production medusa start",
    "migrate:pre": "dotenv -e .env.pre -- medusa db:migrate",
    "migrate:pro": "dotenv -e .env.pro -- medusa db:migrate",
    "seed:pre": "dotenv -e .env.pre -- medusa seed -f ./data/seed.json"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.9.0",
    "@medusajs/cache-redis": "2.9.0",
    "@medusajs/event-bus-redis": "2.9.0",
    "@medusajs/framework": "^2.9.0",
    "@medusajs/inventory": "2.9.0",
    "@medusajs/medusa": "2.9.0",
    "@medusajs/payment-stripe": "2.9.0",
    "@medusajs/workflows-sdk": "2.9.0",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "algoliasearch": "^5.35.0",
    "awilix": "^8.0.1",
    "pg": "^8.13.0",
    "stripe": "^18.4.0",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@medusajs/cli": "^2.9.0",
    "@medusajs/test-utils": "2.9.0",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^10.0.0",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
